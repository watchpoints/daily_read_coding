* from nn1 x
where exists (select /*+ qb_name(x2x)  LEADING(a c b d)*/  1 
from nn1 a, nn2 b, nn1 c, nn2 d   where a.c1 = b.d1
and c.c2 = d.d1) order by x.c1 desc, x.c2 asc, x.c3 desc;
c1	c2	c3
