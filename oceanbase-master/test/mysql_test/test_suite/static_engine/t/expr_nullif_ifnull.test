let $cnt=22;
let $left=1;
while ($left <= $cnt)
{
  let $right=1;
  while ($right <= $cnt)
  {
    eval select c$left, c$right, nullif(c$left, c$right), ifnull(c$left, c$right) from t1;
    eval select c$left, nullif(c$left, c$left) from t1;

    inc $right;
  }
  inc $left;
}

